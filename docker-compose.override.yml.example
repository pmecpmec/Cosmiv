# Docker Compose Override Example
# Copy this to docker-compose.override.yml for local customizations
# This file is automatically loaded by docker-compose

version: '3.8'

services:
  backend:
    ports:
      - "8000:8000"
    environment:
      - LOG_LEVEL=DEBUG
    volumes:
      - ./backend/src:/app  # Development hot-reload

  worker:
    environment:
      - LOG_LEVEL=DEBUG
    volumes:
      - ./backend/src:/app  # Development hot-reload

