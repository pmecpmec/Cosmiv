# ============================================
# AIDIT Frontend Environment Configuration
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# --------------------------------------------
# API Configuration
# --------------------------------------------
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# --------------------------------------------
# Feature Flags
# --------------------------------------------
VITE_ENABLE_SOCIAL_FEATURES=true
VITE_ENABLE_BILLING=true
VITE_ENABLE_ANALYTICS=true

# --------------------------------------------
# Stripe (Public Keys Only)
# --------------------------------------------
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_XXXXXXXXXXXXXXXXXXXXX

# --------------------------------------------
# Analytics (User Secret)
# --------------------------------------------
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=YOUR_MIXPANEL_TOKEN_HERE

# --------------------------------------------
# Sentry (Frontend Monitoring)
# --------------------------------------------
VITE_SENTRY_DSN=https://XXXXXXXXXXXXXXXXXXXXX@sentry.io/XXXXXXX

# --------------------------------------------
# Development
# --------------------------------------------
VITE_DEBUG=false
VITE_LOG_LEVEL=info
